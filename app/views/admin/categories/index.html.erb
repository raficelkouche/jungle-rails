<h1>Categories</h1>
<br><br>
<div class="panel panel-default products">
    <table class="table table-bordered table-striped">
      <thead>
        <tr>
          <th>Category</th>
          <th>Created at</th>
          <th>Updated at</th>
        </tr>
      </thead>
      <tbody>
        <% @categories.each do |element| %>
          <tr>
            <td><%= element.name %></td>
            <td><%= element.created_at %></td>
            <td><%= element.updated_at %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    </div>
    <%= link_to "Add Category", new_admin_category_path, class: "btn btn-primary" %>
